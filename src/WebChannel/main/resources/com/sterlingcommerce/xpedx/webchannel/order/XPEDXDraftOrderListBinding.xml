<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
  <comment>Binding entries for draft order list action.</comment>
  <entry key="pageNumber.toString()">xpedxDraftOrderList:/Page/@PageNumber</entry>
  <entry key="pageSetToken">xpedxDraftOrderList:/Page/@PageSetToken</entry>
  <entry key="orderByAttribute">xpedxDraftOrderList:/Page/API/Input/XPEDXOrderListView/OrderBy/Attribute/@Name</entry>
  <entry key="orderDesc">xpedxDraftOrderList:/Page/API/Input/XPEDXOrderListView/OrderBy/Attribute/@Desc</entry>
  <entry key="WCContext.storefrontId">xpedxDraftOrderList:/Page/API/Input/XPEDXOrderListView/@EnterpriseCode</entry>
  <entry key="WCContext.customerId">xpedxDraftOrderList:/Page/API/Input/XPEDXOrderListView/@BuyerOrganizationCode</entry>
  <!-- RUgrani: Adding page size -->
  <entry key="recordPerPage">xpedxDraftOrderList:/Page/@PageSize</entry>
  
</properties>
