<!DOCTYPE struts PUBLIC
		"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
		"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="xpedx.punchout" namespace="/punchout" extends="wcDefault">
		<result-types>
			<result-type name="json" class="com.googlecode.jsonplugin.JSONResult" />
		</result-types>
		
		<action name="cxmlLogin" class="com.sterlingcommerce.xpedx.webchannel.punchout.PunchoutCxmlLoginAction">
			<result>/xpedx/jsp/punchout/punchoutlogin.jsp</result>
			<result name="error">/xpedx/jsp/punchout/punchoutTimeout.jsp</result>
		</action>
		
		<action name="ociLogin" class="com.sterlingcommerce.xpedx.webchannel.punchout.PunchoutOciLoginAction">
			<result>/xpedx/jsp/punchout/punchoutlogin.jsp</result>
		</action>
		
		<action name="punchoutTimeout" class="com.sterlingcommerce.webchannel.core.WCAction">
			<result>/xpedx/jsp/punchout/punchoutTimeout.jsp</result>
		</action>
		
		<action name="punchoutShipToSearchAction" class="com.sterlingcommerce.xpedx.webchannel.punchout.PunchoutShipToSearchAction">
			<result name="punchout" >/xpedx/jsp/punchout/PunchoutLandingPage.jsp</result>
			<result name="noShipTo" >/xpedx/jsp/punchout/punchoutNoShipTo.jsp</result>
		</action>
		
		<action name="PunchoutUtilities" class="com.sterlingcommerce.webchannel.core.WCAction">
			<result>/xpedx/jsp/punchout/PunchoutUtilities.jsp</result>
		</action>
		<action name="ajaxCreatePunchoutOciDataParam" class="com.sterlingcommerce.xpedx.webchannel.punchout.AjaxCreatePunchoutOciDataParam">
			<result type="json"></result>
		</action>
		
	</package>
</struts>
