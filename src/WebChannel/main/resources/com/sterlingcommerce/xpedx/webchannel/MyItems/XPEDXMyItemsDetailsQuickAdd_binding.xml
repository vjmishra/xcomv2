<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<!-- START - For adding product into the list -->
	<comment>Binding entries for Creating My Items List action</comment>
	<entry key="name">XPEDXMyItemsDetailsCreate:/XPEDXMyItemsItems/@Name</entry>
	<entry key="desc">XPEDXMyItemsDetailsCreate:/XPEDXMyItemsItems/@Desc</entry>
	<entry key="qty">XPEDXMyItemsDetailsCreate:/XPEDXMyItemsItems/@Qty</entry>
	<entry key="jobId">XPEDXMyItemsDetailsCreate:/XPEDXMyItemsItems/@JobId</entry>
	<entry key="order">XPEDXMyItemsDetailsCreate:/XPEDXMyItemsItems/@ItemOrder</entry>
	<entry key="listKey">XPEDXMyItemsDetailsCreate:/XPEDXMyItemsItems/@MyItemsListKey,XPEDXMyItemsQuickAddMulti:/XPEDXMyItemsList/@MyItemsListKey,XPEDXMyItemsQuickAddMulti:/XPEDXMyItemsList/XPEDXMyItemsItemsList/@MyItemsListKey</entry>
	<entry key="itemId">XPEDXMyItemsDetailsCreate:/XPEDXMyItemsItems/@ItemId,XPEDXMyItemsDetailsAddFromCatalog:/Item/@ItemID</entry>
	<entry key="validateItemId">XPEDXMyItemsListValidateItem:/Item/@ItemID</entry>
	<entry key="itemType">XPEDXMyItemsDetailsCreate:/XPEDXMyItemsItems/@ItemType</entry>
	<entry key="uomId">XPEDXMyItemsDetailsCreate:/XPEDXMyItemsItems/@UomId</entry>
	<entry key="purchaseOrder">XPEDXMyItemsDetailsCreate:/XPEDXMyItemsItems/@ItemPoNumber</entry>
	<entry key="WCContext.loggedInUserId">XPEDXMyItemsDetailsCreate:/XPEDXMyItemsItems/@Createuserid</entry>
	<entry key="WCContext.loggedInUserName">XPEDXMyItemsDetailsCreate:/XPEDXMyItemsItems/@Createusername</entry>
	<entry key="WCContext.storefrontId">XPEDXMyItemsListValidateItem:/Item/@CallingOrganizationCode, XPEDXMyItemsListGetMPC:/Item/@CallingOrganizationCode, xpedx-customer-getExtnFieldsForCustomerPart:/Customer/@OrganizationCode</entry>
	<!-- END - For adding product into the list -->
	
	<!-- START - Validate XPEX Items -->
	<entry key="itemManufacturerId">XPEDXMyItemsDetailsAddFromCatalog:/Item/PrimaryInformation/@ManufacturerItem</entry>

	<entry key="#parameters.sfId">XPEDXMyItemsDetailsAddFromCatalog:/Item/@CallingOrganizationCode</entry>
	<!-- END - Validate XPEX Items -->
	
	<!-- START - Customer Part -->
	<!-- Not used as this mashup is called with valuemap
	entry key="itemCustomerPartId">XPEDXMyItemsListGetCustomersPart:/XPXItemcustXref/@CustomerItemNumber</entry>
	-->
	<entry key="WCContext.customerId">xpedx-customer-getExtnFieldsForCustomerPart:/Customer/@CustomerID,XPEDXMyItemsDetailsAddFromCatalog:/Item/CustomerInformation/@CustomerID, XPEDXMyItemsListValidateItem:/Item/CustomerInformation/@CustomerID</entry>
	<!-- END - Customer Part -->
	
	<!-- START Validation MPC Code -->
	<entry key="itemMpcId">XPEDXMyItemsDetailsAddFromCatalog:/Item/Extn/@ExtnMpc</entry>
	<!-- END Validation MPC Code -->
	
	<!-- START - For adding all products into the list -->
	<entry key="orderedNames">XPEDXMyItemsQuickAddMulti:/XPEDXMyItemsList/XPEDXMyItemsItemsList/XPEDXMyItemsItems[]/@Name</entry>
	<entry key="orderedDescs">XPEDXMyItemsQuickAddMulti:/XPEDXMyItemsList/XPEDXMyItemsItemsList/XPEDXMyItemsItems[]/@Desc</entry>
	<entry key="orderedQtys">XPEDXMyItemsQuickAddMulti:/XPEDXMyItemsList/XPEDXMyItemsItemsList/XPEDXMyItemsItems[]/@Qty</entry>
	<entry key="orderedJobIds">XPEDXMyItemsQuickAddMulti:/XPEDXMyItemsList/XPEDXMyItemsItemsList/XPEDXMyItemsItems[]/@JobId</entry>
	<entry key="orders">XPEDXMyItemsQuickAddMulti:/XPEDXMyItemsList/XPEDXMyItemsItemsList/XPEDXMyItemsItems[]/@ItemOrder</entry>
	<entry key="orderedItemIds">XPEDXMyItemsQuickAddMulti:/XPEDXMyItemsList/XPEDXMyItemsItemsList/XPEDXMyItemsItems[]/@ItemId</entry>
	<entry key="orderedItemTypes">XPEDXMyItemsQuickAddMulti:/XPEDXMyItemsList/XPEDXMyItemsItemsList/XPEDXMyItemsItems[]/@ItemType</entry>
	<entry key="orderedUoms">XPEDXMyItemsQuickAddMulti:/XPEDXMyItemsList/XPEDXMyItemsItemsList/XPEDXMyItemsItems[]/@UomId</entry>
	<entry key="orderedPurchaseOrders">XPEDXMyItemsQuickAddMulti:/XPEDXMyItemsList/XPEDXMyItemsItemsList/XPEDXMyItemsItems[]/@ItemPoNumber</entry>
	<entry key="listKeyAsList">XPEDXMyItemsQuickAddMulti:/XPEDXMyItemsList/XPEDXMyItemsItemsList/XPEDXMyItemsItems[]/@MyItemsListKey</entry>
	<entry key="createUserIdList">XPEDXMyItemsQuickAddMulti:/XPEDXMyItemsList/XPEDXMyItemsItemsList/XPEDXMyItemsItems[]/@Createuserid</entry>
	<entry key="createUserNameList">XPEDXMyItemsQuickAddMulti:/XPEDXMyItemsList/XPEDXMyItemsItemsList/XPEDXMyItemsItems[]/@Createusername</entry>
	<entry key="createUserName">XPEDXMyItemsQuickAddMulti:/XPEDXMyItemsList/@Createusername</entry>
	<!-- END - For adding all product into the list -->
	
</properties>
