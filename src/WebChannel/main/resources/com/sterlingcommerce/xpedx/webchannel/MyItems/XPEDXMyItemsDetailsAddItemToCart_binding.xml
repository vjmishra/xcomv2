<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
   <comment>Binding entries for adding line items to a draft order (via Quick Add).</comment>
    
    <entry key="WCContext.storefrontId">addToCartGetCompleteItemList:/Item/@CallingOrganizationCode,draftOrderGetCustomerLineFields:/Customer/@OrganizationCode</entry>
    <entry key="productID">addToCartGetCompleteItemList:/Item/@ItemID</entry>
    <entry key="associationTypesOfInterest">addToCartGetCompleteItemList:/Item/ItemAssociationTypeList/ItemAssociationType[]/@Type</entry>
	<entry key="WCContext.customerId">addToCartGetCompleteItemList:/Item/CustomerInformation/@CustomerID,draftOrderGetCustomerLineFields:/Customer/@CustomerID</entry>
    <entry key="currency">addToCartGetCompleteItemList:/Item/@Currency</entry>
    
    <entry key="orderHeaderKey">me_draftOrderAddOrderLines:/Order/@OrderHeaderKey</entry>
    <entry key="draftOrderflag">me_draftOrderAddOrderLines:/Order/@DraftOrderFlag</entry>
    <entry key="orderedProductIDs">me_draftOrderAddOrderLines:/Order/OrderLines/OrderLine[]/Item/@ItemID</entry>
    <entry key="orderedProductUOMs">me_draftOrderAddOrderLines:/Order/OrderLines/OrderLine[]/Item/@UnitOfMeasure</entry>
    <entry key="ExtnCustLineAccNo">me_draftOrderAddOrderLines:/Order/OrderLines/OrderLine[]/Extn/@ExtnCustLineAccNo</entry>
    <entry key="ExtnCustomerPONo">me_draftOrderAddOrderLines:/Order/OrderLines/OrderLine[]/@CustomerPONo</entry>
    <entry key="ExtnCustomerLinePONo">me_draftOrderAddOrderLines:/Order/OrderLines/OrderLine[]/@CustomerLinePONo</entry>
    <entry key="ExtnCustLineField1">me_draftOrderAddOrderLines:/Order/OrderLines/OrderLine[]/Extn/@ExtnCustLineField1</entry>
    <entry key="ExtnCustLineField2">me_draftOrderAddOrderLines:/Order/OrderLines/OrderLine[]/Extn/@ExtnCustLineField2</entry>
    <entry key="ExtnCustLineField3">me_draftOrderAddOrderLines:/Order/OrderLines/OrderLine[]/Extn/@ExtnCustLineField3</entry>
    <entry key="orderedLineTypeList">me_draftOrderAddOrderLines:/Order/OrderLines/OrderLine[]/@LineType</entry>
    <entry key="orderedProductClasses">me_draftOrderAddOrderLines:/Order/OrderLines/OrderLine[]/Item/@ProductClass</entry>
    <entry key="orderedQuantities">me_draftOrderAddOrderLines:/Order/OrderLines/OrderLine[]/OrderLineTranQuantity/@OrderedQty</entry>
 	<entry key="transactionalUOMs">me_draftOrderAddOrderLines:/Order/OrderLines/OrderLine[]/OrderLineTranQuantity/@TransactionalUOM</entry> 
    <entry key="recordPendingChanges">me_draftOrderAddOrderLines:/Order/PendingChanges/@RecordPendingChanges</entry>
    <entry key="isEditNewline">me_draftOrderAddOrderLines:/Order/OrderLines/OrderLine[]/Extn/@ExtnEditOrderFlag</entry>
</properties>