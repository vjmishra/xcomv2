    <entry key="#parameters.selectedDeliveryMethods">xpedx_me_changeOrderLineDetails:/Order/OrderLines/OrderLine[]/@DeliveryMethod,xpedx_checkout_changeOrderLineDetails:/Order/OrderLines/OrderLine[]/@DeliveryMethod</entry>
