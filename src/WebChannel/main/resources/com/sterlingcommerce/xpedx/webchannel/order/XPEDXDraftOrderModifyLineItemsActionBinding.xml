    <entry key="#parameters.orderLineCustLineField1">xpedx_me_changeOrderLineDetails:/Order/OrderLines/OrderLine[]/Extn/@ExtnCustLineField1,xpedx_checkout_changeOrderLineDetails:/Order/OrderLines/OrderLine[]/Extn/@ExtnCustLineField1</entry>
    <entry key="#parameters.orderLineCustLineField2">xpedx_me_changeOrderLineDetails:/Order/OrderLines/OrderLine[]/Extn/@ExtnCustLineField2,xpedx_checkout_changeOrderLineDetails:/Order/OrderLines/OrderLine[]/Extn/@ExtnCustLineField2</entry>
    <entry key="#parameters.orderLineCustLineField3">xpedx_me_changeOrderLineDetails:/Order/OrderLines/OrderLine[]/Extn/@ExtnCustLineField3,xpedx_checkout_changeOrderLineDetails:/Order/OrderLines/OrderLine[]/Extn/@ExtnCustLineField3</entry>
