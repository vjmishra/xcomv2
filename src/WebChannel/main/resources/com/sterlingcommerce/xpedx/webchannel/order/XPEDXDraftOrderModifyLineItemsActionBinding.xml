    <entry key="recordPendingChanges">xpedx_me_changeOrderLineDetails:/Order/PendingChanges/@RecordPendingChanges,xpedx_checkout_changeOrderLineDetails:/Order/PendingChanges/@RecordPendingChanges</entry>
