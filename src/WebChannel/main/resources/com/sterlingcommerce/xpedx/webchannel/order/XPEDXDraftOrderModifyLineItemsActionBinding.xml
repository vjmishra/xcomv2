    <entry key="#parameters.orderLineQuantities">xpedx_me_changeOrderLineDetails:/Order/OrderLines/OrderLine[]/OrderLineTranQuantity/@OrderedQty,xpedx_checkout_changeOrderLineDetails:/Order/OrderLines/OrderLine[]/OrderLineTranQuantity/@OrderedQty</entry>    
