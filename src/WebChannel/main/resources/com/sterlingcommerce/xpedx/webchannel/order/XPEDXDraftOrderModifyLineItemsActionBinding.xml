<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>Binding entries for changing draft order line information.</comment>
    
    <entry key="#parameters.orderHeaderKey">xpedx_me_changeOrderLineDetails:/Order/@OrderHeaderKey,xpedx_checkout_changeOrderLineDetails:/Order/@OrderHeaderKey</entry>
    <entry key="#parameters.orderName">xpedx_me_changeOrderLineDetails:/Order/@OrderName,xpedx_checkout_changeOrderLineDetails:/Order/@OrderName</entry>
