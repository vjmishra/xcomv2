			<param name="mashups">createCustomerQuickLink</param>
			<param name="xmlBindingFile">com/sterlingcommerce/xpedx/webchannel/profile/user/XPEDXCreateQL_binding.xml</param>
			<result name="redirect" type="redirectAction">
