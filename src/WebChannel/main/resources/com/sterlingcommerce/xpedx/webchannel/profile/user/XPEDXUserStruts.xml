			<param name="resourceId">/swc/profile/ManageUserProfile</param>
			<result name="success">/xpedx/jsp/profile/user/XPEDXAuthorizeCustomerLocations.jsp</result>
		</action>
		<!-- End Added For Jira - On Add we call this action  -->
		
