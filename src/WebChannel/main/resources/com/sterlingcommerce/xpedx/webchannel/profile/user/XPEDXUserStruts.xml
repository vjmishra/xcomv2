			<param name="xmlBindingFile">com/sterlingcommerce/xpedx/webchannel/profile/user/XPEDXGetCustomerLocationsBinding.xml</param> 
			<result name="success">/xpedx/jsp/profile/user/XPEDXUserLocations.jsp</result>				
		</action>

