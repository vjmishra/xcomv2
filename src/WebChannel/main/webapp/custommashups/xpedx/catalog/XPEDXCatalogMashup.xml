							<CategoryList>
						<Category CategoryID="" CategoryPath="" Count="" Description=""
							SequenceNo="" ShortDescription="">
							<ChildCategoryList>
								<Category CategoryPath="" Count="" Description=""
									SequenceNo="" ShortDescription="">
								</Category>
							</ChildCategoryList>
						</Category>
					</CategoryList>
						</Item>
					</ItemList>
				</CatalogSearch>
			</Template>
		</API>
