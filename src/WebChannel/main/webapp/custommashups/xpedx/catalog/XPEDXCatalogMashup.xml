	<mashup id='xpedxCatalogSearch' transactional='true' mashuptype='XAPI'
		endpoint='CATALOG' description="Search and Navigation in Catalog">
		<classInformation
			name="com.sterlingcommerce.ui.web.platform.mashup.SCUIXAPIMashup" />
		<API Name="searchCatalogIndex">
			<Input>
				<SearchCatalogIndex CallingOrganizationCode=""
					CategoryDepth="" DisplayLocalizedFieldInLocale="" SortField=""
					SortDescending="" PageSize="" PageNumber="" CategoryPath="">
					<Item GetAvailabilityFromCache='Y' Currency=""
						ShowFinalSupersededItemOnly="Y" ExcludeChildItemsOfModelItems="Y">
						<CustomerInformation CustomerID="" />
						<AdditionalCatalogIndexInformationList>
							<AdditionalCatalogIndexInformation
								IndexFieldName="customerNumberPlusPartNumber" />
							<AdditionalCatalogIndexInformation
								IndexFieldName="showNormallyStockedItems" />							
						</AdditionalCatalogIndexInformationList>
					</Item>
				</SearchCatalogIndex>
			</Input>
			<Template>
				<CatalogSearch CallingOrganizationCode=""
					CategoryDepth="" DisplayLocalizedFieldInLocale="" PageNumber=""
					PageSize="" SortDescending="" SortField="" TotalHits="" TotalPages="">
					<SortFieldList>
						<SortField IndexFieldName="" />
					</SortFieldList>
					<CategoryDomain AttributeName="" CategoryDomain=""
						Description="" IsClassification="" ShortDescription="" />
					<CategoryList>
						<Category CategoryID="" CategoryPath="" Count="" Description=""
							SequenceNo="" ShortDescription="">
							<ChildCategoryList>
								<Category CategoryPath="" Count="" Description=""
									SequenceNo="" ShortDescription="">
								</Category>
							</ChildCategoryList>
						</Category>
					</CategoryList>
					<FacetList>
						<ItemAttribute IndexFieldName="" ItemAttributeName=""
							IsProvidedFilter="">
							<Attribute AllowMultipleValues="" AllowedValueLength="" AttributeDomainID="" AttributeGroupID="" 
								AttributeID="" AttributeKey="" AttributePostFix="" DataType="" DerivedFromAttributeKey=""
								IsAllowedValueDefined="" IsValueMandatory="" LongDescription="" OrganizationCode="" SequenceNo="" 
								ShortDescription="" /> 
 
							<AssignedValueList>
								<AssignedValue Count="" Value="" IsProvidedFilter="" />
							</AssignedValueList>
						</ItemAttribute>
					</FacetList>
					<ItemList Currency="">
						<Item IsItemSuperseded="" ItemID="" ItemKey="" UnitOfMeasure="">
							<Extn/>
							<PrimaryInformation Description="" IsValid=""
								ShortDescription="" KitCode="" IsConfigurable=""
								IsPreConfigured="" IsModelItem="" IsShippingAllowed=""
								IsPickupAllowed="" MinOrderQuantity="" ImageID="" ImageLabel=""
								ImageLocation="" ConfigurationKey="" ConfigurationModelName="" ManufacturerItem=""/>
							
							 <ComputedPrice BundleTotal="" ListPrice=""
